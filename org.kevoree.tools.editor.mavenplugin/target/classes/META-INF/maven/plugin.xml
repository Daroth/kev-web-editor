<?xml version="1.0" encoding="UTF-8"?>
<plugin>
  <name>Kevoree Web Editor :: Maven Plugin</name>
  <description>Sonatype helps open source projects to set up Maven repositories on https://oss.sonatype.org/</description>
  <groupId>org.kevoree.tools</groupId>
  <artifactId>rjs-maven-plugin</artifactId>
  <version>1.0.0-SNAPSHOT</version>
  <goalPrefix>rjs</goalPrefix>
  <isolatedRealm>false</isolatedRealm>
  <inheritedByDefault>true</inheritedByDefault>
  <mojos>
    <mojo>
      <goal>wrap</goal>
      <description></description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <implementation>org.kevoree.tools.editor.mavenplugin.RequireJSisationPlugin</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
      <parameters>
        <parameter>
          <name>input</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>Input JS file name to wrap with requireJS</description>
        </parameter>
        <parameter>
          <name>kotlinInput</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>Kotlin JS merged file path</description>
        </parameter>
        <parameter>
          <name>project</name>
          <type>org.apache.maven.project.MavenProject</type>
          <required>true</required>
          <editable>false</editable>
          <description>The maven project.</description>
        </parameter>
        <parameter>
          <name>rjsKotlinDep</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>Generated Kotlin dependence in RequireJS format define([&apos;kotlin/&lt;b&gt;kotlin-merged&lt;/b&gt;&apos;], function (Kotlin) {...});</description>
        </parameter>
      </parameters>
      <configuration>
        <project implementation="org.apache.maven.project.MavenProject">${project}</project>
        <rjsKotlinDep implementation="java.lang.String" default-value="kotlin-merged"/>
      </configuration>
    </mojo>
  </mojos>
  <dependencies>
    <dependency>
      <groupId>com.google.javascript</groupId>
      <artifactId>closure-compiler</artifactId>
      <type>jar</type>
      <version>v20130722</version>
    </dependency>
    <dependency>
      <groupId>args4j</groupId>
      <artifactId>args4j</artifactId>
      <type>jar</type>
      <version>2.0.16</version>
    </dependency>
    <dependency>
      <groupId>com.google.guava</groupId>
      <artifactId>guava</artifactId>
      <type>jar</type>
      <version>14.0.1</version>
    </dependency>
    <dependency>
      <groupId>com.google.protobuf</groupId>
      <artifactId>protobuf-java</artifactId>
      <type>jar</type>
      <version>2.4.1</version>
    </dependency>
    <dependency>
      <groupId>org.json</groupId>
      <artifactId>json</artifactId>
      <type>jar</type>
      <version>20090211</version>
    </dependency>
    <dependency>
      <groupId>com.google.code.findbugs</groupId>
      <artifactId>jsr305</artifactId>
      <type>jar</type>
      <version>1.3.9</version>
    </dependency>
    <dependency>
      <groupId>commons-io</groupId>
